{
  "plugins": [
    "prettier"
  ],
  "parser": "babel-eslint",
  "extends": [
    "prettier",
    "eslint:recommended"
  ],
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "rules": {
    "prettier/prettier": "warn",
    "no-unused-vars": ["warn", { "args": "after-used", "ignoreRestSiblings": true  }]
  },
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true
    }
  }
}